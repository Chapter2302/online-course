    <script>
        loadMiniCart()

        function loadMiniCart() {
            let cartCourses = JSON.parse(localStorage.getItem('cart'))
            if (cartCourses) {
                document.getElementsByClassName('product_qun')[0].textContent = cartCourses.length
                document.getElementsByClassName('qty minicart')[0].textContent = cartCourses.length + ' items'
                let price = 0
                cartCourses.map((item) => {
                    price = price + item.Price
                })

                document.getElementsByClassName('price minicart')[0].textContent = '$' + price + '.00'
            }
        }
    </script>
